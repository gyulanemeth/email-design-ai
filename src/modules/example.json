        "padding": "10px 20px",
        "font-size": "16px",
        "font-weight": "bold",
        "text-transform": "uppercase"
      },
      "button-secondary": {
        "background": "#FFFFFF",
        "color": "#FF0000",
        "border": "1px solid #FF0000",
        "border-radius": "5px",
        "padding": "10px 20px",
        "font-size": "16px",
        "font-weight": "bold",
        "text-transform": "uppercase"
      }
    },
